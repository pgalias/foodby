{
  "name": "@foodby/common-components-ui",
  "version": "0.1.2",
  "license": "UNLICENSED",
  "main": "dist/main.es.js",
  "types": "dist/main.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "start-storybook -p 6006",
    "build": "tsc --noEmit && vite build",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@foodby/commons": "0.0.3",
    "autoprefixer": "^10.3.4",
    "clsx": "^1.1.1",
    "leaflet": "^1.7.1",
    "leaflet-geosearch": "^3.5.0",
    "lodash.debounce": "^4.0.8",
    "lodash.uniqueid": "^4.0.1",
    "react": "^17.0.0",
    "react-leaflet": "^3.2.1",
    "tailwindcss": "^2.2.15"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@rollup/plugin-typescript": "^8.2.5",
    "@storybook/addon-actions": "^6.4.0-alpha.34",
    "@storybook/addon-essentials": "^6.4.0-alpha.34",
    "@storybook/addon-links": "^6.4.0-alpha.34",
    "@storybook/react": "^6.4.0-alpha.34",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/leaflet": "^1.7.5",
    "@types/leaflet-geosearch": "^3.0.0",
    "@types/lodash": "^4.14.172",
    "@types/node": "^16.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "babel-loader": "^8.2.2",
    "eslint": "^7.30.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^4.12.2",
    "jest": "^27.0.6",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "storybook-builder-vite": "^0.0.12",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2",
    "vite": "^2.5.4"
  },
  "peerDependencies": {
    "react": "^17.0.0"
  },
  "jest": {
    "clearMocks": true,
    "testEnvironment": "jsdom",
    "preset": "ts-jest",
    "coverageDirectory": "../coverage",
    "collectCoverageFrom": [
      "src/**/*.(j|t)s?(x)",
      "!src/**/*.mock.(j|t)s?(x)",
      "!src/**/test*",
      "!src/**/mock/**.*"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "rootDir": "src",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "moduleNameMapper": {
      "\\.css$": "<rootDir>/__mocks__/styleMock.ts",
      "\\.svg$": "<rootDir>/__mocks__/svgMock.ts"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.ts"
    ]
  }
}
